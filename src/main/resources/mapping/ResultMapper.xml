<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.zjnu.hmz.dao.ResultMapper">
    <resultMap id="BaseResultMap" type="cn.zjnu.hmz.model.Result">
        <constructor>
            <arg column="image" jdbcType="VARCHAR" javaType="java.lang.String"/>
            <arg column="option" jdbcType="INTEGER" javaType="java.lang.Integer"/>
            <arg column="url" jdbcType="VARCHAR" javaType="java.lang.String"/>
        </constructor>
    </resultMap>

    <select id="selectDB" resultMap="BaseResultMap">
   SELECT
   *
   FROM
   result
    </select>
</mapper>